      <Property ovf:userConfigurable="true" ovf:value="Compute-ResourcePool" ovf:type="string" ovf:key="VSPHERE_RESOURCE_POOL">
        <Label>vSphere Resource Pool</Label>
        <Description>The resource pool where VMs are created.</Description>
